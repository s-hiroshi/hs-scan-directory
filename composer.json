{
    "name": "s-hiroshi/hs-scan-directory",
    "description": "Script to scan directories made symfony4 console command.",
    "type": "symfony/console",
    "require": {
        "symfony/console": "^4.2",
        "symfony/event-dispatcher": "^4.2",
        "symfony/yaml": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
      "psr-4": {
        "SH\\Scan\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "SH\\Scan\\": [
          "tests/",
          "tests/Fake"
        ]
      }
    },
